		"vitest": "^4.0.3"
	},
	"dependencies": {
		"@tauri-apps/api": "^2.9.0",
		"@tauri-apps/plugin-dialog": "^2.4.2",
		"bits-ui": "^2.14.1",
		"lucide-svelte": "^0.548.0",
